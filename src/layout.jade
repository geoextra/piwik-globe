- var app= { name: 'Piwik-Globe', repository: 'https://github.com/staticshowdown/ss15-develofy'}

doctype html
html
  head
    block title
      title Piwik Globe
    
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    
    link(href="./third-party/bootstrap/dist/css/bootstrap.min.css", rel='stylesheet')
    link(href="./third-party/bootstrap-material-design/dist/css/material-wfont.min.css", rel='stylesheet')
    link(href="./third-party/bootstrap-material-design/dist/css/ripples.min.css", rel='stylesheet')
    
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
      |       
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
    
    block head
    
  body
    block content
    
    script(src='./third-party/jquery/dist/jquery.min.js')
    script(src='./third-party/bootstrap/dist/js/bootstrap.min.js')
    script(src='./third-party/bootstrap-material-design/dist/js/material.min.js')
    script(src='./third-party/bootstrap-material-design/dist/js/ripples.min.js')
    script(src='./third-party/system.js/build/system.min.js')
    
    block footer